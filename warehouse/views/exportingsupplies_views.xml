<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_exporting_tree" model="ir.ui.view">
            <field name="name">vnitpro.exporting.tree</field>
            <field name="model">vnitpro.exporting</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="warehouse_id"/>
                    <field name="compute_name"/>

                </tree>
            </field>
        </record>
        <record id="view_exporting_form" model="ir.ui.view">
            <field name="name">vnitpro.exporting.form</field>
            <field name="model">vnitpro.exporting</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group col="4" name="main">
                            <field name="code"/>
                            <field name="name"/>
                            <field name="warehouse_id"/>
                            <field name="compute_name"/>
                        </group>
                        <group string="Details">
                            <field name="details_ids" nolabel="1">
                                <tree editable="bottom">
                                    <field name="supplies_id"/>
                                    <field name="groupsupplies_id"/>
                                    <field name="quantity_id"/>
                                </tree>
                            </field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_exporting_search" model="ir.ui.view">
            <field name="name">vnitpro.exporting.search</field>
            <field name="model">vnitpro.exporting</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="warehouse_id"/>
                    <field name="compute_name"/>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_exporting_view">
            <field name="name">Exporting</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">vnitpro.exporting</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_exporting_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>