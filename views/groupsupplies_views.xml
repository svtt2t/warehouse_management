<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_groupsupplies_tree" model="ir.ui.view">
            <field name="name">groupsupplies.tree</field>
            <field name="model">groupsupplies</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="view_groupsupplies_form" model="ir.ui.view">
            <field name="name">groupsupplies.form</field>
            <field name="model">groupsupplies</field>
            <field name="priority" eval="4"/>
            <field name="arch" type="xml">
                <form>

                    <sheet>
                        <group col = "4" name="main">
                            <field name="code" colspan="2"/>
                            <field name="name" colspan="2"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_groupsupplies_search" model="ir.ui.view">
            <field name="name">groupsupplies.search</field>
            <field name="model">groupsupplies</field>
            <field name="priority" eval="4"/>
            <field name="arch" type="xml">
                <search>
                    <field name="code"/>
                    <field name="name"/>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_groupsupplies_view">
            <field name="name">group_supplies</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">groupsupplies</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_groupsupplies_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>